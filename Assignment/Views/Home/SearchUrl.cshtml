    ViewData["Title"] = "SearchUrl";
}

<h2>SearchUrl</h2>

<h4>User</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SearchUrl">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
